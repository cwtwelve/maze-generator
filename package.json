{
	"name": "maze-generator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "concurrently -p \"name\" -n \"PRETTY,CLIENT\" -c \"green,blue\" --kill-others \"npm run prettier-watch\" \"npm run develop\"",
		"develop": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"pretty": "prettier --write \"**/*\"",
		"prettier-watch": "onchange \"app/**/*\" \"src/**/*\" -- prettier --write --ignore-unknown {{changed}}"
	},
	"dependencies": {
		"@types/node": "^20.4.4",
		"@types/react": "^18.2.15",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"autoprefixer": "^10.4.14",
		"classnames": "^2.3.2",
		"concurrently": "^8.2.0",
		"daisyui": "^3.3.1",
		"eslint": "^8.45.0",
		"eslint-config-next": "^13.4.12",
		"eslint-config-prettier": "^8.8.0",
		"lodash": "^4.17.21",
		"next": "^13.4.12",
		"onchange": "^7.1.0",
		"postcss": "^8.4.27",
		"prettier": "^3.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hook-form": "^7.45.2",
		"sass": "^1.64.1",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.1.6"
	},
	"devDependencies": {
		"@types/lodash": "^4.14.195"
	}
}
